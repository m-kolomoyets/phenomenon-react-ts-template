{
    "name": "phenomenon-react-ts-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": ">=16.0.0",
        "yarn": "please-use-npm",
        "npm": ">=7.10.0"
    },
    "scripts": {
        "dev": "vite --open --host",
        "build": "tsc && vite build",
        "preview": "vite preview --open --host",
        "lint:eslint": "eslint --cache --max-warnings 0 --report-unused-disable-directives ",
        "lint:stylelint": "stylelint --cache -q \"**/*.css\"",
        "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,jsx,json,html,css,cjs}\"",
        "lint:prettier:fix": "prettier --write \"**/*.{js,ts,tsx,jsx,json,html,css,cjs}\"",
        "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
        "prepare": "husky install"
    },
    "dependencies": {
        "@tanstack/react-router": "^1.58.3",
        "clsx": "^2.1.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@eslint/compat": "^1.1.1",
        "@eslint/js": "^9.10.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
        "@tanstack/router-devtools": "^1.58.3",
        "@tanstack/router-vite-plugin": "^1.58.4",
        "@types/react": "^18.3.8",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^8.6.0",
        "@typescript-eslint/parser": "^8.6.0",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.36.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "globals": "^15.9.0",
        "husky": "^9.1.6",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "rollup-plugin-visualizer": "^5.12.0",
        "stylelint": "^16.9.0",
        "stylelint-config-standard": "^36.0.1",
        "typescript": "^5.6.2",
        "vite": "^5.4.6",
        "vite-plugin-eslint": "^1.8.1"
    }
}
