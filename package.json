{
    "name": "phenomenon-react-ts-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": ">=22.0.0",
        "yarn": "please-use-npm",
        "npm": ">=10.9.0"
    },
    "scripts": {
        "dev:vite": "vite --open --host",
        "dev:tsc": "tsc --build --watch",
        "dev": "run-p dev:*",
        "build": "vite build",
        "preview": "vite preview --open --host",
        "lint:eslint": "eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,tsx,cjs,mjs}\"",
        "lint:stylelint": "stylelint --cache -q \"**/*.css\"",
        "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,json,html,css,cjs,mjs}\"",
        "lint": "run-s lint:*",
        "fix:prettier": "prettier --write \"**/*.{js,ts,tsx,json,html,css,cjs,mjs}\"",
        "prepare": "husky"
    },
    "dependencies": {
        "@svg-use/react": "^0.4.3",
        "@tanstack/react-router": "^1.99.0",
        "clsx": "^2.1.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.7.1",
        "@commitlint/config-conventional": "^19.7.1",
        "@eslint/js": "^9.19.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
        "@svg-use/vite": "^0.1.3",
        "@tanstack/router-devtools": "^1.99.0",
        "@tanstack/router-vite-plugin": "^1.99.0",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.1.0",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.5.1",
        "prettier": "^3.4.2",
        "rollup-plugin-visualizer": "^5.14.0",
        "stylelint": "^16.14.1",
        "stylelint-config-standard": "^37.0.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.22.0",
        "vite": "^6.0.11",
        "vite-plugin-eslint2": "^5.0.3"
    }
}
